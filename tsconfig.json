{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["es2017","dom"],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": ["cypress"],
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmit": true
  },
  "include": ["cypress/**/*.ts"]
}